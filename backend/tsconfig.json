{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "typeRoots": ["./src/types", "./node_modules/@types"],
    "types": ["node", "express"],
  },
  "include": ["src"]
}